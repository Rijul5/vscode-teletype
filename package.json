{
	"name": "vscode-teletype",
	"displayName": "vscode-teletype",
	"description": "VS Code Teletype extension for Creating CoEditing skeleton for Eclipse Che and Theia",
	"types": "dist/index.d.ts",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.35.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.join-portal"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.join-portal",
				"title": "Join Portal"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production && npm run compile",
		"compile": "webpack --mode none && tsc -p ./",
		"watch": "webpack --mode none --watch && tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"awesome-typescript-loader": "^5.2.1",
		"source-map-loader": "^0.2.4",
		"tslint": "^5.12.1",
		"typescript": "^3.5.2",
		"vscode": "^1.1.28",
		"webpack": "^4.34.0",
		"webpack-cli": "^3.3.4",
		"@atom/teletype-client": "github:Rijul5/teletype-client",
		"@atom/teletype-server": "^0.18.1",
		"deep-equal": "^1.0.1",
		"dotenv": "^4.0.0",
		"electron": "2.0.0",
		"electron-mocha": "^4.0.3"
	},
	"dependencies": {
		"@atom/teletype-crdt": "^0.9.0",
		"electron-webrtc-patched": "^0.3.0",
		"event-kit": "^2.5.3",
		"google-protobuf": "^3.8.0",
		"node-fetch": "^2.6.0",
		"socketcluster-client": "^13.0.0",
		"teletype": "^0.2.0",
		"uuid": "^3.2.1",
		"webrtc-adapter": "~6.1"
	}
}
